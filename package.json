{
  "name": "practice",
  "version": "1.0.0",
  "description": "This project was bootstrapped with Fastify-CLI.",
  "main": "src/app.ts",
  "scripts": {
    "clean": "rm -rf dist",
    "build": "yarn tsc",
    "watch": "tsc -w --preserveWatchOutput",
    "start": "yarn build && fastify start -l info dist/src/app.js",
    "dev": "concurrently -k -p \"[{name}]\" -n \"TypeScript,App\" -c \"yellow.bold,cyan.bold\" \"yarn watch\" \"fastify start --ignore-watch=.ts$ -w -l info -P dist/src/app.js\"",
    "test": "yarn build && vitest run",
    "test:watch": "yarn build && vitest"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@fastify/auth": "^5.0.3",
    "@fastify/autoload": "^6.0.0",
    "@fastify/env": "^5.0.3",
    "@fastify/helmet": "^13.0.2",
    "@fastify/sensible": "^6.0.0",
    "@zodyac/zod-mongoose": "^4.2.0",
    "fastify": "^5.0.0",
    "fastify-cli": "^7.4.1",
    "fastify-plugin": "^5.0.0",
    "fastify-type-provider-zod": "^6.1.0",
    "mongoose": "^8.19.3",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@types/node": "^24.0.8",
    "c8": "^10.1.2",
    "concurrently": "^9.0.0",
    "fastify-tsconfig": "^3.0.0",
    "ts-node": "^10.4.0",
    "typescript": "~5.9.2",
    "vitest": "^4.0.9"
  }
}
